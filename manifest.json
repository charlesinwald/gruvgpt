{
  "manifest_version": 3,
  "name": "ChatGPT Gruvbox Theme",
  "description": "A Gruvbox theme for ChatGPT",
  "version": "1.0",
  "icons": {
    "16": "logo/GruvboxLogo16.png",
    "48": "logo/GruvboxLogo48.png",
    "128": "logo/GruvboxLogo128.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://*.openai.com/*"],
      "css": ["gruvbox.css"]
    }
  ]
}

